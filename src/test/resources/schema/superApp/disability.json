{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "status": { "type": "boolean" },
    "message": { "type": "string" },
    "data": {
      "type": "object",
      "properties": {
        "code": { "type": "string" },
        "group": { "type": "integer" },
        "start_date": { "type": "string", "format": "date" },
        "end_date": {
          "anyOf": [
            { "type": "string", "format": "date" },
            { "type": "null" }
          ]
        }
      },
      "required": ["code", "end_date", "group", "start_date"]
    }
  },
  "required": ["data", "message", "status"]
}
